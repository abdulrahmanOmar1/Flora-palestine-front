// open-close init
function initOpenClose() {
	function addRow(box,holder,visible){
        var label;
        var row = jQuery('<div class="row" />');
        var clone;
        if (box.hasClass("acolor")) {
            label = box.find('strong');
            clone = label.clone();
            clone = jQuery("<label class='chk-label-active'><span class='term-name'>"+ clone.html() + "</span></label>");
        }
        else {
            label = box.find('label');
            if(label.length){
                clone = label.clone();
                clone.find('input, .chk-area').remove();
                clone.removeAttr('for');
            }
        }
        if (label.length) {
            if(!visible) row.hide();
            row.append(clone);
            row.appendTo(holder);
            box.data('append',row);
        }
    }
	function showRow(box){
		if(box.data('append')){
			box.data('append').show();
		}
	}
	function hideRow(box){
		if(box.data('append')){
			box.data('append').hide();
		}
	}
	
	jQuery('#nav').openClose({
		hideOnClickOutside: true,
		activeClass: 'active',
		opener: '.open-nav',
		slider: '.mobile-nav',
		animSpeed: 400,
		effect: 'slide'
	});
	jQuery('#sidebar .open-close').openClose({
		//hideOnClickOutside: true,
		activeClass: 'active',
		opener: '.opener',
		slider: '.slide',
		onInit: function(that){
			var selectedBox = that.holder.find('.selected-box');
			if(selectedBox.length){
				that.selectedBox = selectedBox;
				that.slider.find('.row:has(input:checkbox), label:has(input:checkbox)').each(function(){
					var row = jQuery(this);
					var input = row.find('input:checkbox');
					addRow(row,that.selectedBox,input.is(':checked'));
					input.on('change',function(){
						if(input.is(':checked')) showRow(row);
						else hideRow(row);
					})
				})
				if(that.holder.hasClass(that.options.activeClass)) that.selectedBox.hide();
			}
		},
		animStart: function(direction,that){
			if(direction && that.selectedBox){
				that.selectedBox.slideUp(that.options.animSpeed);
			}
			else if(!direction && that.selectedBox){
				that.selectedBox.slideDown(that.options.animSpeed);
			}
		},
		animSpeed: 400,
		effect: 'slide'
	});
	jQuery('.level1 .video-tools').openClose({
		hideOnClickOutside: true,
		activeClass: 'active',
		opener: '.more',
		slider: '.presentation-block',
		animSpeed: 400,
		effect: 'slide'
	});
}

// fancybox modal popup init
function initLightbox() {
	var win = jQuery(window);
	jQuery('a.lightbox, a[data-rel*="lightbox"]').each(function(){
		var link = jQuery(this);
		link.attr('rel', link.attr('data-rel')).fancybox({
			padding: 0,
			margin: 0,
			cyclic: false,
			autoScale: true,
			overlayShow: true,
			overlayOpacity: 0.90,
			overlayColor: '#191919',
			titlePosition: 'inside',
			onComplete: function(box) {
				if(link.attr('href').indexOf('#') === 0) {
					jQuery('#fancybox-content').find('a.close').unbind('click.fb').bind('click.fb', function(e){
						jQuery.fancybox.close();
						e.preventDefault();
					});
				}
				var wrapLightbox = jQuery('#fancybox-wrap');
				var holderBox = jQuery('#fancybox-content');
				var contentBox = holderBox.find('> div > div');

				win.unbind('resize.fb scroll.fb');
				
				function setPosition(){
                    if (contentBox.length > 0) {
                        if(contentBox.outerWidth() < win.width()){
                            wrapLightbox.stop().css({left: (win.width() - contentBox.outerWidth()) / 2})
                        }
                        else{
                            wrapLightbox.stop().css({left: 0})
                        }
                        if(contentBox.outerHeight() < win.height()){
                            wrapLightbox.stop().css({top: win.scrollTop() + (win.height() - contentBox.outerHeight()) / 2})
                        }
                        else{
                            wrapLightbox.stop().css({top: win.scrollTop()})
                        }
                    }
				}
				
				setTimeout(function(){
					setPosition();
				},200)
				
				win.bind('resize orientationchange',function(){
					if(contentBox.outerWidth() != holderBox.width()){
						holderBox.add(wrapLightbox).css({width: contentBox.outerWidth()})
					}
					setPosition();
				})
			}
		});
	});
}

