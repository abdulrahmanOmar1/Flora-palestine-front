// initCompare
function initCompare() {
	var activeHolderClass = 'active-compare';
	var body = jQuery('body');
	jQuery('.item-section').each(function(event) {
		var holder = jQuery(this);
		var items = holder.find('.item');

		items.each(function(i) {
			var item = jQuery(this).attr('num', i);
			var openLink = item.find('.open-link');
			var linkPopup1 = item.find('.popup2 .lightbox');
			var linkPopup2 = item.find('.popup3 .lightbox');

			openLink.on('click', function(e) {
				e.preventDefault();
				if (!holder.hasClass(activeHolderClass)) {
					holder.addClass(activeHolderClass);
					body.addClass('footer-active');
				}
			});
			item.on({
				mouseenter: function(e) {
						e.preventDefault();
						if (holder.hasClass(activeHolderClass)) {
							if (!item.hasClass('popup-active')) {
								item.addClass('second-item');
							}
						}
					},
				mouseleave: function(e) {
					e.preventDefault();
					if (holder.hasClass(activeHolderClass)) {
						item.removeClass('second-item');
					}
				},
			});
			linkPopup1.on('click', function(event) {
				event.preventDefault();
				var link = jQuery(this);
				var linkHref = link.attr('href');
				var targetModal = jQuery(linkHref);
				var targetGallery = targetModal.find('.gallery');
				var galleryAPI = targetGallery.data('ScrollGallery');
				if(galleryAPI) {
					var linkIndex = link.parents('.item').attr('num');
					var origAnimSpeed = galleryAPI.options.animSpeed;
					galleryAPI.options.animSpeed = 0;
					galleryAPI.numSlide(linkIndex);
					galleryAPI.options.animSpeed = origAnimSpeed;
				}
				body.removeClass('footer-active');
				items.removeClass('popup-active');
				items.removeClass('second-item');
				item.parents('.item-section').removeClass('active-compare');
			});
			linkPopup2.on('click', function(event) {
				event.preventDefault();
				var link = jQuery(this);
				var linkHref = link.attr('href');
				var targetModal = jQuery(linkHref);
				var targetGalleryLeft = targetModal.find('.col-left .gallery');
				var targetGalleryRight = targetModal.find('.col-right .gallery');
				var galleryLeftAPI = targetGalleryLeft.data('ScrollGallery');
				var galleryRightAPI = targetGalleryRight.data('ScrollGallery');

				if(galleryRightAPI) {
					var linkIndex = link.parents('.item').attr('num');
					var origAnimSpeed = galleryRightAPI.options.animSpeed;
					galleryRightAPI.options.animSpeed = 0;
					galleryRightAPI.numSlide(linkIndex);
					galleryRightAPI.options.animSpeed = origAnimSpeed;
				}
				if(galleryLeftAPI) {
					var linkIndex = link.parents('.item-section ').find('.popup-active').attr('num');
					var origAnimSpeed = galleryLeftAPI.options.animSpeed;
					galleryLeftAPI.options.animSpeed = 0;
					galleryLeftAPI.numSlide(linkIndex);
					galleryLeftAPI.options.animSpeed = origAnimSpeed;
				}
				body.removeClass('footer-active');
				items.removeClass('popup-active');
				items.removeClass('second-item');
				item.parents('.item-section').removeClass('active-compare');
			});
		});
	});
}

function initSlider(){
	jQuery('.data-line').each(function(){
		var holder = jQuery(this);
		var maxVal = parseInt(holder.find('input.max').val());
		var minVal = parseInt(holder.find('input.min').val());
		var curVal = parseInt(holder.find('input.cur-val').val());
		
		var outputCurrent = holder.find('.ui-slider-handle em');
		
		holder.slider({
			isRTL: true,
			range: "max",
			min: minVal,
			max: maxVal,
			value: curVal,
			slide: function(event,ui){
				if(outputCurrent.length) outputCurrent.text(ui.value);
			}
		})
		if(outputCurrent.length) outputCurrent.text(curVal);
	})
}

function initLayout(){
	var lightboxClass = 'lightbox';
	var links = jQuery('a.mobile-lightbox');
	var isTouchDevice = ('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch;
	
	function attachLightbox(){
		var win = jQuery(window);
		links.each(function(){
			var link = jQuery(this);
			link.trigger('unbindOpenClose');
			link.attr('rel', link.attr('data-rel')).fancybox({
				padding: 0,
				margin: 0,
				cyclic: false,
				autoScale: true,
				overlayShow: true,
				overlayOpacity: 0.90,
				overlayColor: '#191919',
				titlePosition: 'inside',
				onComplete: function(box) {
					if(link.attr('href').indexOf('#') === 0) {
						jQuery('#fancybox-content').find('a.close').unbind('click.fb').bind('click.fb', function(e){
							jQuery.fancybox.close();
							e.preventDefault();
						});
					}
					var wrapLightbox = jQuery('#fancybox-wrap');
					var holderBox = jQuery('#fancybox-content');
					var contentBox = holderBox.find('> div > div');
					win.unbind('resize.fb scroll.fb');
					
					function setPosition(){
						if(contentBox.outerWidth() < win.width()){
							wrapLightbox.stop().css({left: (win.width() - contentBox.outerWidth()) / 2})
						}
						else{
							wrapLightbox.stop().css({left: 0})
						}
						if(contentBox.outerHeight() < win.height()){
							wrapLightbox.stop().css({top: win.scrollTop() + (win.height() - contentBox.outerHeight()) / 2})
						}
						else{
							wrapLightbox.stop().css({top: win.scrollTop()})
						}
					}
					
					setTimeout(function(){
						setPosition();
					},200)
					
					win.bind(isTouchDevice ? 'orientationchange' : 'resize orientationchange',function(){
						if(win.width() > 767){
							jQuery.fancybox.close();
						}
						if(contentBox.outerWidth() != holderBox.width()){
							holderBox.add(wrapLightbox).css({width: contentBox.outerWidth()})
						}
						setPosition();
					})
				}
			});
			link.bind('unbindLightbox',function(){
				link.unbind('click');
				if(link.data('fancybox')) link.data('fancybox',null);
			});
		});
	}
	
	function attachOpenClose(){
		links.each(function(){
			jQuery(this).trigger('unbindLightbox');
			jQuery(this).trigger('bindOpenClose');
		})
	}
	
	// handle layout resize
	ResponsiveHelper.addRange({
		'..767': {
			on: function() {
				attachLightbox();
			}
		},
		'768..': {
			on: function() {
				attachOpenClose();
			}
		}
	});
}

function showSelectedRegions() {
    var labels = jQuery('#modal-place .cols label');
    labels.each(function() {
        var me=jQuery(this);
        var map_cover = jQuery('#modal-place .map .region-cover'+ me.data('region')).show().css("opacity", 1);
        if (me.hasClass('chk-label-active')) {
            map_cover.show();
        }
        else {
            map_cover.hide();
        }
    });
}

function initSelectedRows(){ // Lightbox - area selection (magdir page)
	jQuery('.selected-area').each(function(){
		var holder = jQuery(this);
		var outputBox = holder.find('.output-row');
		var rows = holder.find('.row-check');
		if(outputBox.length){
			function addLabel(label){
				var span = jQuery('<span />');
				if(outputBox.children().length){
					var em = jQuery('<em />').text(', ');
					em.appendTo(span);
					label.data('separate',em);
				}
				var clone = label.clone();
				clone.appendTo(span);
				span.appendTo(outputBox);
				label.data('box',span);
			}
			function removeLabel(label){
				label.data('box').remove();
				refreshBoxes();
			}
			function refreshBoxes(){
				if(outputBox.children().length && outputBox.children().eq(0).find('em').length) {
					outputBox.children().eq(0).find('em').remove();
				}
			}
			
			rows.each(function(){
				var row = jQuery(this);
				var label = row.find('label');
				var checkbox = row.find('input:checkbox');
				if(checkbox.is(':checked')) addLabel(label);
				checkbox.bind('change',function(){
					if(checkbox.is(':checked')) addLabel(label);
					else removeLabel(label);
				})
			})
		}

        var columns = holder.find(".col");
        columns.each(function() {
           var col=jQuery(this);
           col.find("h3").click(function() {
               var checkedBoxes = col.find('input:checkbox:checked');
               var uncheckedBoxes = col.find('input:checkbox:not(:checked)');
               if (checkedBoxes.length > 0 && uncheckedBoxes.length == 0) {
                   col.find('.chk-area').removeClass("chk-checked").addClass("chk-unchecked");
                   checkedBoxes.each (function() {jQuery(this).attr("checked",false).trigger("change").siblings("label").removeClass("chk-label-active");});
               }
               else {
                   col.find('.chk-area').removeClass("chk-unchecked").addClass("chk-checked");
                   uncheckedBoxes.each (function() {jQuery(this).attr("checked",true).trigger("change").siblings("label").addClass("chk-label-active");});
               }
           });
        });

        var labels = jQuery('#modal-place .cols label');
        labels.each(function() {
            var me = jQuery(this);
            me.hover(function() {
                jQuery('#modal-place .map .region-cover' + me.data("region")).show().css("opacity","0.5");
            });
            me.mouseout(function() {
                showSelectedRegions();
            });
        });

        var checkboxes = jQuery ('#modal-place input:checkbox');
        checkboxes.each(function () {
            jQuery(this).change(function () {
                showSelectedRegions();
            });
        });
        showSelectedRegions();
        jQuery ("#place-area-tab").click(function(e) {
           switchToPlaceTab("area");
            e.stopPropagation();
        });
        jQuery ("#place-coord-tab").click(function(e) {
            switchToPlaceTab("coord");
            e.stopPropagation();
        });
        jQuery (".area-link, .open.area-link").click(function(e) {
            switchToPlaceTab("area");
        })

       // jQuery ("#open-close-location").trigger('unbindOpenClose').click(function(e) { switchToPlaceTab("area");e.stopPropagation();});

        jQuery (".coord-link").click(function(e) {
            switchToPlaceTab("coord");
        });

	})

}
var MapsInitialized = false;
function switchToPlaceTab(tab) {
    var newTab = "#place-" + tab;
    var oldTab = "#place-";
    if (tab == "area") oldTab += "coord";  else oldTab +="area";
    jQuery (newTab).show();
    jQuery (oldTab).hide();
    jQuery (newTab + "-tab").addClass ("selected");
    jQuery (oldTab + "-tab").removeClass ("selected");
    if (tab == "coord") {
        if (!MapsInitialized) {
            initializeMaps();
            MapsInitialized = true;
        }
        google.maps.event.trigger(document.getElementById("map-canvas"),'resize');
    }
}

