// page init
jQuery(function( $ ){

	var usePhoneGalleryBox = $("body").width() < 640;

	initTabs();
	jQuery('input, textarea').placeholder();
	jcf.customForms.replaceAll();
	initOpenClose();
	initLayout();
	initLightbox();
	initPopups();
	//initInputs();
	initSameHeight();
	initCustomHover();
	initMultiColumns();

	// No carsousel for phone, instead, swipebox is used (see flora.js)
	// simply show the first image.
	// Since the images are width scaled proportionally to a hardcoded height
	// of 385, the width may be smaller than the screen width, so we need to adjust
	// the mask to hide all other images and center it nicely.
	// See single-flora_plant.php for hardcoded height.
	if ( usePhoneGalleryBox ) {
		initGalleryBoxForPhone();
	}

	initCarousel( { initGalleryBox: ! usePhoneGalleryBox } );
	initSelectedRows();
	initSlider();
	initCompare();
});

// Function that's used instead of initCarousel() in phones
// Sets the width of the gallery box to display the first slide nicely.
// Only that one first slide is displayed on phones.
function initGalleryBoxForPhone() {

	var // div.gallery-box only exists in single-flora_plant.php
		galleryBox = jQuery( 'div.gallery-box' ),

		// Should be the first one, rtl/ltr shouldn't matter
		currentlyDisplayedSlide = galleryBox.find( 'div.slide' ).eq( 0 );

		firstImage = currentlyDisplayedSlide.find( 'img' ),

		getWidth = function () {
			return firstImage.width();
		},

		setGalleryBoxWidth = function () {

			galleryBox
				.width( getWidth() )
				.css( {
					// horizontal centering
					margin: '0 auto'
				} );

			// Display slide - mimics the action done by initCarousel()
			currentlyDisplayedSlide.animate( { opacity: 1 } );

		};


	if ( getWidth() ) {
		setGalleryBoxWidth();
	} else {
		firstImage.on( 'load', setGalleryBoxWidth );
	}

}
