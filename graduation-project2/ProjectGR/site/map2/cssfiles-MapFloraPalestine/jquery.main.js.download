// scroll gallery init
function initCarousel( options ) {
	var ie8 = jQuery.support.opacity ? false : true;

	if ( options.initGalleryBox ) {

		jQuery('div.gallery-box').scrollGallery({
			mask: 'div.mask',
			slider: 'div.slideset',
			slides: 'div.slide',
			btnPrev: 'a.next',
			btnNext: 'a.prev',
			handleTouch: false,
			swipeMobile: true,
			onInit: function(that){
				var isTouchDevice = ('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch;
				var frame = that.gallery.find('.handler');
				var shadow = frame.find('span');
				var switcherImages = that.gallery.find('.thumbnails img');
				if(frame.length && switcherImages.length){
					var body = jQuery('body');
					that.frame = frame;
					that.switcherImages = switcherImages;
					var parentSwitcher = switcherImages.parent();
					that.setSizeFrame = function(flag,onChange){
						var firstItem = that.slides.filter(function(){
							return jQuery(this).offset().left < that.mask.offset().left + that.mask.width();
						}).eq(0);
						var lastItem = that.slides.filter(function(){
							return jQuery(this).offset().left + jQuery(this).outerWidth(true) > that.mask.offset().left && jQuery(this).offset().left <= that.mask.offset().left;
						}).eq(0);
						if (!lastItem.length) {
							lastItem = that.slides.eq(4);
						}

						var firstIndex = that.slides.index(firstItem);
						var lastIndex = that.slides.index(lastItem);
						var visibleWidthFirst = visibleWidthLast = 100;
						if(firstItem.offset().left + firstItem.outerWidth(true) > that.mask.offset().left + that.mask.width()){
							visibleWidthFirst = (firstItem.offset().left + firstItem.outerWidth(true) - that.mask.offset().left - that.mask.width()) / firstItem.outerWidth(true) * 100;
						}

						if(!(typeof(lastItem.offset()) === 'undefined')) {
	                        if(lastItem.offset().left < that.mask.offset().left){
	                            visibleWidthLast = (lastItem.offset().left + lastItem.outerWidth(true) - that.mask.offset().left) / lastItem.outerWidth(true) * 100;
	                        }
	                    }

						var curRight = Math.ceil(parentSwitcher.offset().left + parentSwitcher.width() - that.switcherImages.eq(firstIndex).offset().left - that.switcherImages.eq(firstIndex).outerWidth(true)) +
										(visibleWidthFirst != 100 ? that.switcherImages.eq(firstIndex).outerWidth(true) / 100 * visibleWidthFirst : 0);

						var curWidth = that.switcherImages.eq(firstIndex).offset().left + that.switcherImages.eq(firstIndex).outerWidth(true) -
									(visibleWidthFirst != 100 ? that.switcherImages.eq(firstIndex).outerWidth(true) / 100 * visibleWidthFirst : 0) -
									(that.switcherImages.eq(lastIndex).offset().left + (visibleWidthLast != 100 ? that.switcherImages.eq(lastIndex).outerWidth(true) -
									(that.switcherImages.eq(lastIndex).outerWidth(true) / 100 * visibleWidthLast) : 0));

						if(that.mask.width() < body.width()){
							curWidth = 176;
						}

						//curWidth = Math.ceil(curWidth) - 8;
						if(parseInt(that.slider.css('marginRight')) <= that.maxOffset){
							curRight = parentSwitcher.outerWidth() - curWidth;
						}

						that.slides.animate( { opacity: 1 } );

						// Ensure frame right doesn't overflow, copied from eventPress handler below.
						// Also updates the state of the next and previous buttons.
						if( curRight > 0 && curRight > parentSwitcher.width() - that.frame.outerWidth(true)) {
							curRight = parentSwitcher.width() - that.frame.outerWidth(true)
							that.gallery.addClass('end');
						} else {
							that.gallery.removeClass('end');
						};

						that.frame.stop().animate({
							width:  Math.ceil(curWidth) - 3,
							right: 	curRight

						},{duration: flag ? 0 : that.options.animSpeed, complete: function () {

							// Update slider, copied from eventPress handler below.
							var percent = curRight / (parentSwitcher.width() - that.frame.outerWidth(true)) * 100;
							var offsetList = (that.slider.width() - that.mask.width()) / 100 * percent;

							if ( offsetList > 0 ) {
								that.slider.css({marginRight: - offsetList});
							}

							// Make sure we're rtl, all code here assumes that.
							if ( that.mask.width() <= that.slider.width() ) {
							that.mask.css( 'direction', 'rtl' );
							}
							that.frame.css( 'left', 'auto' );

							that.refreshState();

						} })

					}
					jQuery(window).bind('load',function(){
						that.setSizeFrame(true);
					}).bind('resize orientationchange',function(){
						that.setSizeFrame(true);
					})

					var eventPress, eventMove, eventRelease;
					if(navigator.pointerEnabled) {
						eventPress = 'pointerdown';
						eventMove = 'pointermove';
						eventRelease = 'pointerup';
					} else if(navigator.msPointerEnabled) {
						eventPress = 'MSPointerDown';
						eventMove = 'MSPointerMove';
						eventRelease = 'MSPointerUp';
					} else {
						eventPress = isTouchDevice ? 'touchstart' : 'mousedown';
						eventMove = isTouchDevice ? 'touchmove' : 'mousemove';
						eventRelease = isTouchDevice ? 'touchend' : 'mouseup';
					}

					var dragFlag = false;
					that.frame.bind(isTouchDevice ? 'touchend' : 'click', function(e) {
						// ignore if dragged
						if(dragFlag) {
							return;
						}

						// calculate offsets
						var mouseX = e.pageX || e.originalEvent.changedTouches[0].pageX,
							thumbnailsOffset = parentSwitcher.offset().left,
							innerOffsetLeft = mouseX - thumbnailsOffset,
							maxOffset = parentSwitcher.width() - that.frame.outerWidth()+1;
							innerPercent = 1 - innerOffsetLeft / parentSwitcher.width();

						// move frame
						var frameOffset = innerPercent * parentSwitcher.width() - that.frame.outerWidth()/2;
						if(frameOffset < 0) frameOffset = 0;
						else if(frameOffset > maxOffset){
							frameOffset = maxOffset;
							//that.gallery.addClass('end');
						} else {
							//that.gallery.removeClass('end');
						};
						that.frame.stop().animate({right: frameOffset},500);

						// move content
						/*
						var offsetList = (that.slider.width() - that.mask.width()) * innerPercent;
						that.slider.stop().animate({marginRight: - offsetList}, 500);
						that.currentStep = parseInt(offsetList / that.mask.width());
						that.refreshState();
						*/


						var percent = frameOffset / (parentSwitcher.width() - that.frame.outerWidth(true)) * 100;
						var offsetList = (that.slider.width() - that.mask.width()) / 100 * percent;
						that.slider.stop().animate({marginRight: - offsetList}, 500, function(){
							that.refreshState();
					});
						that.currentStep = parseInt(offsetList / that.mask.width());
					});

					that.frame.bind(eventPress,function(e){
						var touchStart = e.pageX || e.originalEvent.changedTouches[0].pageX;
						var currentOffset = parseInt(that.frame.css('right'));
						body.bind(eventMove,function(e){
							var moveValue = (e.pageX || e.originalEvent.changedTouches[0].pageX) -  touchStart;
							var currentValue = currentOffset - moveValue;
							if(currentValue < 0) currentValue = 0;
							else if(currentValue > parentSwitcher.width() - that.frame.outerWidth(true)) {
								currentValue = parentSwitcher.width() - that.frame.outerWidth(true)
								that.gallery.addClass('end');
							} else {
								that.gallery.removeClass('end');
							};
							that.frame.css({right: currentValue});

							var percent = currentValue / (parentSwitcher.width() - that.frame.outerWidth(true)) * 100;
							var offsetList = (that.slider.width() - that.mask.width()) / 100 * percent;
							that.slider.css({marginRight: - offsetList});

							dragFlag = true;
							that.currentStep = parseInt(offsetList / that.mask.width());
							that.refreshState();
						}).bind(eventRelease,function(){
							body.unbind(eventMove);
							setTimeout(function() {
								dragFlag = false;
							}, 100);
						})
						return false;
					}).bind(eventRelease,function(){
						body.unbind(eventMove);
						//return false;
					});

				}
			},
			onChange: function(that){
				that.gallery.removeClass('end');
				that.setSizeFrame(false,true);
			},
			circularRotation: false,
			rtl: true,
			autoRotation: false,
			switchTime: 3000,
			animSpeed: 500
		});

	}

